        <div class="c-get-in-touch__form">
          <form class="c-form c-form--lg" id="wf-form-contactUs" name="wf-form-contactUs" data-name="contactUs" method="post" action="//identity.docuwalk.com/api/leads/new" redirect="/pricing" data-redirect="/pricing">
            <div class="c-form__inner">
              <div class="c-form__row c-form__row--2">
                <div class="c-form__col">
                  <input type="text" maxlength="70" name="firstName" data-name="firstName" placeholder="Name" class="c-field w-input" required />
                </div>
                <div class="c-form__col">
                  <input type="email" maxlength="70" name="email" data-name="email" placeholder="Email" class="c-field w-input" required />
                </div>
              </div>
              <div class="c-form__row c-form__row--1">
                <div class="c-form__col">
                  <input type="text" maxlength="70" name="company" data-name="company" placeholder="Company" class="c-field w-input" required />
                </div>
              </div>
              <div class="c-form__row c-form__row--1">
                <div class="c-form__col">
                  <textarea class="c-field w-input" maxlength="500" name="message" data-name="message" placeholder="Message" spellcheck="false" required></textarea>
                </div>
              </div>
              <div class="c-form__row c-form__row--1">
                <div class="c-form__col c-form__col--flex-end">
                  <input type="submit" value="Submit" data-wait="Please wait..." class="c-btn c-btn--light-blue c-btn--min w-button" />
                  <input type="text" value="contactUs" maxlength="20" name="formName" hidden required readonly />
                </div>
              </div>
            </div>
          </form>
        </div>
